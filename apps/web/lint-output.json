o-cermont-primary-600 border-4 border-white dark:border-gray-800 shadow-lg transition-all duration-200 group-hover:brightness-90\">\r\n            <span class=\"text-4xl font-bold text-white\">\r\n              {{ getInitials() }}\r\n            </span>\r\n          </div>\r\n        }\r\n\r\n        <!-- Overlay with camera icon -->\r\n        <div class=\"absolute inset-0 flex items-center justify-center rounded-full bg-black/40 opacity-0 group-hover:opacity-100 transition-opacity duration-200\">\r\n          <svg class=\"w-8 h-8 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M3 9a2 2 0 012-2h.93a2 2 0 001.664-.89l.812-1.22A2 2 0 0110.07 4h3.86a2 2 0 011.664.89l.812 1.22A2 2 0 0018.07 7H19a2 2 0 012 2v9a2 2 0 01-2 2H5a2 2 0 01-2-2V9z\"/>\r\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 13a3 3 0 11-6 0 3 3 0 016 0z\"/>\r\n          </svg>\r\n        </div>\r\n\r\n        <!-- Edit badge -->\r\n        <div class=\"absolute bottom-0 right-0 flex h-10 w-10 items-center justify-center rounded-full bg-cermont-primary-600 border-4 border-white dark:border-gray-800 shadow-lg group-hover:scale-110 transition-transform duration-200\">\r\n          <svg class=\"w-5 h-5 text-white\" fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\r\n            <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15.232 5.232l3.536 3.536m-2.036-5.036a2.5 2.5 0 113.536 3.536L6.5 21.036H3v-3.572L16.732 3.732z\"/>\r\n          </svg>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Hidden file input -->\r\n      <input\r\n        #fileInput\r\n        type=\"file\"\r\n        class=\"hidden\"\r\n        accept=\"image/jpeg,image/jpg,image/png,image/webp\"\r\n        (change)=\"onFileSelected($event)\"\r\n      />\r\n\r\n      <!-- Loading Indicator -->\r\n      @if (uploading()) {\r\n        <div class=\"absolute inset-0 flex items-center justify-center rounded-full bg-black/60\">\r\n          <div class=\"spinner border-white\"></div>\r\n        </div>\r\n      }\r\n\r\n      <!-- Error Message -->\r\n      @if (error()) {\r\n        <div class=\"absolute -bottom-16 left-1/2 -translate-x-1/2 w-64 text-center\">\r\n          <p class=\"text-sm text-error-600 dark:text-error-400 bg-white dark:bg-gray-900 rounded-lg px-4 py-2 shadow-lg\">\r\n            {{ error() }}\r\n          </p>\r\n        </div>\r\n      }\r\n    </div>\r\n  `\r\n})\r\nexport class AvatarEditorComponent {\r\n    private readonly uploadService = inject(UploadService);\r\n\r\n    @Input() userName: string = '';\r\n    @Input() set avatar(value: string | null) {\r\n        this.currentAvatar.set(value);\r\n    }\r\n\r\n    @Output() avatarChanged = new EventEmitter<string>();\r\n\r\n    currentAvatar = signal<string | null>(null);\r\n    uploading = signal(false);\r\n    error = signal<string | null>(null);\r\n\r\n    getInitials(): string {\r\n        if (!this.userName) return '?';\r\n        return this.userName\r\n            .split(' ')\r\n            .map(n => n[0])\r\n            .join('')\r\n            .toUpperCase()\r\n            .slice(0, 2);\r\n    }\r\n\r\n    async onFileSelected(event: Event): Promise<void> {\r\n        const input = event.target as HTMLInputElement;\r\n        const file = input.files?.[0];\r\n\r\n        if (!file) return;\r\n\r\n        this.error.set(null);\r\n\r\n        const validation = this.uploadService.validateImageFile(file);\r\n        if (!validation.valid) {\r\n            this.error.set(validation.error || 'Archivo inv├ílido');\r\n            setTimeout(() => this.error.set(null), 3000);\r\n            return;\r\n        }\r\n\r\n        try {\r\n            const preview = await this.uploadService.createImagePreview(file);\r\n            this.currentAvatar.set(preview);\r\n\r\n            this.uploading.set(true);\r\n            this.uploadService.uploadAvatar(file).subscribe({\r\n                next: (response) => {\r\n                    this.currentAvatar.set(response.url);\r\n                    this.avatarChanged.emit(response.url);\r\n                    this.uploading.set(false);\r\n                },\r\n                error: (err) => {\r\n                    this.error.set(err.error?.message || 'Error al subir la imagen');\r\n                    this.uploading.set(false);\r\n                    setTimeout(() => this.error.set(null), 3000);\r\n                }\r\n            });\r\n        } catch (err) {\r\n            this.error.set('Error al procesar la imagen');\r\n            setTimeout(() => this.error.set(null), 3000);\r\n        }\r\n\r\n        input.value = '';\r\n    }\r\n}\r\n","usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\cards\\card-with-icon\\card-icon-one\\card-icon-one.component.html","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\cards\\card-with-icon\\card-icon-one\\card-icon-one.component.ts","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\cards\\card-with-icon\\card-icon-two\\card-icon-two.component.html","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermo