\n    }\r\n  `\r\n})\r\nexport class ConfirmationModalComponent {\r\n  @Input() show = false;\r\n  @Input() title = 'Confirmar acci├│n';\r\n  @Input() message = '┬┐Est├ís seguro de realizar esta acci├│n?';\r\n  @Input() confirmText = 'Confirmar';\r\n  @Input() cancelText = 'Cancelar';\r\n  @Input() variant: 'danger' | 'warning' | 'info' = 'danger';\r\n  @Output() confirmed = new EventEmitter<void>();\r\n  @Output() cancelled = new EventEmitter<void>();\r\n\r\n  get confirmButtonClass(): string {\r\n    const classes = {\r\n      danger: 'bg-red-600 hover:bg-red-700',\r\n      warning: 'bg-yellow-600 hover:bg-yellow-700',\r\n      info: 'bg-blue-600 hover:bg-blue-700'\r\n    };\r\n    return classes[this.variant];\r\n  }\r\n\r\n  onConfirm(): void {\r\n    this.confirmed.emit();\r\n    this.show = false;\r\n  }\r\n\r\n  onCancel(): void {\r\n    this.cancelled.emit();\r\n    this.show = false;\r\n  }\r\n}\r\n\r\n","usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\data-table\\data-table.component.ts","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\ecommerce\\add-product-form\\add-product-form.component.html","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":[]},{"filePath":"C:\\Users\\camil\\Downloads\\Compressed\\aplicativo_cermont_prueba\\cermont_aplicativo\\apps\\web\\src\\app\\shared\\components\\ecommerce\\add-product-form\\add-product-form.component.ts","messages":[],"suppressedMessages":[],"errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"u